{
  "name": "talks",
  "version": "0.1.1",
  "description": "Presentations for my (me@pavolhejny.com) talks on conferences and meetups.",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "clear": "rm -r src/.next",
    "start": "next ./src -p $PORT",
    "prettier": "prettier --config .prettierrc --write \"{src,.vscode}/**/*.{ts,tsx,json}\"",
    "prettier-watch": "onchange --initial \"**/*\" -- npm run prettier",
    "dev": "next ./src",
    "build": "next build ./src",
    "deploy": " git push heroku master"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:hejny/talks.git"
  },
  "author": "Pavol Hejn√Ω",
  "license": "ISC",
  "homepage": "https://talks.pavolhejny.com",
  "dependencies": {
    "@types/node": "^11.13.9",
    "@types/papaparse": "^4.5.9",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.0.11",
    "configchecker": "^0.4.1",
    "isomorphic-unfetch": "^3.0.0",
    "moment": "^2.24.0",
    "next": "8.1.1-canary.43",
    "onchange": "^3.3.0",
    "papaparse": "^5.0.0",
    "prettier": "github:prettier/prettier",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "typescript": "^3.6.2"
  }
}
